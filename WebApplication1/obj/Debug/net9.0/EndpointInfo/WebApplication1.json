          "admissionDate": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "StudentDTO": {
        "required": [
          "address",
          "age",
          "confirmPassword",
          "dateOfBirth",
          "name",
          "password"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "email": {
            "type": "string",
            "format": "email",
            "nullable": true
          },
          "address": {
            "minLength": 1,
            "type": "string"
          },
          "password": {
            "minLength": 1,
            "type": "string"
          },
          "confirmPassword": {
            "minLength": 1,
            "type": "string"
          },
          "age": {
            "maximum": 18,
            "minimum": 3,
            "type": "integer",
            "format": "int32"
          },
          "admissionDate": {
            "type": "string",
            "format": "date-time"
          },
          "dateOfBirth": {
            "type": "string",
            "format": "date"
          }
        },
        "additionalProperties": false
      }
    },
